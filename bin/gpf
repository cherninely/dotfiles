#!/bin/bash

if [[ "$(git status 2>/dev/null)" ]]; then

    branch="`git rev-parse --abbrev-ref HEAD`"

    # Detect is current branch not a dev
    if [[ $branch != $(git-get-default-branch) ]]; then
        git push origin +$branch;
    else
        echo 'ðŸ‘Š  you in the dev!';
        exit 1;
    fi
else
    echo 'ðŸ˜¥  not git repo'
    exit 1
fi
